@model IEnumerable<ChatBot.Models.Professors>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Professors</h2>

<table class="table table-bordered table-hover">

    <thead>

        <tr>

            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Id</th>
            <th>Phone Number</th>
            <th>Ofice Hours</th>
            <th>Location</th>
        </tr>

    </thead>

    <tbody>
        @foreach (var items in Model)
        {
            <tr>
                <td> @items.fname </td>
                <td> @items.lname</td>
                <td> @items.email</td>
                <td> @items.phone</td>
                <td> @items.officeHours</td>
                <td> @items.location</td>
            </tr>
        }

    </tbody>

</table>

